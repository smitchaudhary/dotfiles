[
  {
    "context": "Editor",
    "bindings": {
      "alt-tab": "pane::ActivateNextItem",
      "alt-shift-tab": "pane::ActivatePrevItem"
    }
  },
  {
    "context": "EmptyPane || SharedScreen || vim_mode == normal",
    "bindings": {
      "ctrl-o": "projects::OpenRecent"
    }
  },
  {
    "context": "Editor && VimControl && vim_mode != waiting && !menu",
    "bindings": {
      "shift-i": "editor::Hover"
    }
  }
]
